<?xml version="1.0" encoding="UTF-8"?>
<container_settings>
	<container name="3gp">
		<video>
			<codec>MPEG4</codec>
			<codec>MPEG2</codec>
			<codec>H264</codec>
		</video>
		<audio>
			<codec>MP3</codec>
			<codec>AAC</codec>
		</audio>
	</container>
	
	<container name="webm">
		<video>
			<codec>VP8</codec>
		</video>
		<audio>
			<codec>VORBIS</codec>
		</audio>
	</container>
	
	<container name="avi">
		<video>
			<codec>MPEG4</codec>
			<codec>MPEG2</codec>
			<codec>H264</codec>
			<codec>XVID</codec>
			<codec>WMV</codec>
		</video>
		<audio>
			<codec>MP3</codec>
			<codec>AC3</codec>
			<codec>WMA</codec>
			<codec>MP2</codec>
		</audio>
	</container>

	<container name="flv">
		<video>
			<codec>H264</codec>
			<codec>AAC</codec>
		</video>
		<audio>	
			<codec>AAC</codec>
			<codec>MP3</codec>
		</audio>
	</container>

	<container name="mkv">
		<video>
			<codec>THEORA</codec>
			<codec>H264</codec>
			<codec>MPEG4</codec>
			<codec>MPEG2</codec>
			<codec>XVID</codec>
		</video>
		<audio>
			<codec>VORBIS</codec>
			<codec>MP3</codec>
			<codec>AAC</codec>
			<codec>AC3</codec>
			<codec>MP2</codec>
			</audio>
	</container>
	
	<container name="mp3">
		<audio>
			<codec>MP3</codec>
		</audio>
	</container>
	
	<container name="mp4">
		<video>
			<codec>MPEG4</codec>
			<codec>H264</codec>
		</video>
		<audio>
			<codec>AAC</codec>
		</audio>
	</container>
	<container name="mpeg">
		<video>
			<codec>MPEG1</codec>
			<codec>MPEG2</codec>
		</video>
		<audio>
			<codec>MP3</codec>
			<codec>MP2</codec>
		</audio>
	</container>
	<container name="ogg">
		<audio>
			<codec>VORBIS</codec>
			<codec>FLAC</codec>
		</audio>
	</container>
	
	<container name="wma">
		<audio>
			<codec>WMA</codec>
		</audio>
	</container>
	<container name="wmv">
		<video>
			<codec>WMV</codec>
		</video>
		<audio>
			<codec>WMA</codec>
			<codec>MP3</codec>
		</audio>
	</container>
</container_settings>
